{
    "layer": "top",
    "modules-left": ["niri/workspaces", "niri/window"],
    "modules-center": [],
    "modules-right": ["battery", "clock"],
    "sway/window": {
        "max-length": 50
    },
    "battery": {
        "format": "{capacity}%"
    },
    "clock": {
        "format-alt": "{:%a, %d. %b  %H:%M}"
    },

    "niri/workspaces": {
        "format": "{value}"
    }
}
